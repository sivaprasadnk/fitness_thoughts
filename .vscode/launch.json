    "version": "0.2.0",
    "configurations": [

        {
            "name": "fitness_thoughts mobile",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "web debug",
            "request": "launch",
            "type": "dart",
            "args": [
                "-d", "chrome"
            ]
        },
        {
            "name": "web release",
            "request": "launch",
            "type": "dart",
            "args": [
                "-d", "chrome", "--release"
            ]
        },
        {
            "name": "fitness_thoughts (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "fitness_thoughts (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}